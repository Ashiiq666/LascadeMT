<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CustomTab" parent="TextAppearance.Design.Tab">
        <item name="textAllCaps">false</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:fontFamily">@font/gilroy_semibold</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="BottomNavCardStyle">
        <item name="cardCornerRadius">25dp</item>
        <item name="cardPreventCornerOverlap">false</item>
        <item name="android:divider">@android:color/transparent</item>
        <item name="android:dividerHeight">0.0px</item>
        <item name="android:clipToPadding">false</item>
        <item name="android:clipChildren">false</item>
    </style>

    <style name="CornerImageView4dp">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">4dp</item>
    </style>
</resources>